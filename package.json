{
  "name": "react-starter-kit",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@types/node": "11.13.0",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "husky": "^1.3.1",
    "prettier": "1.16.4",
    "react": "^16.8.6",
    "react-app-polyfill": "^0.2.2",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "tslint -p ./tsconfig.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "printWidth": 120,
    "parser": "flow",
    "trailingComma": "es5",
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
